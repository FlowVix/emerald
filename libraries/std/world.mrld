





let {
    World,
    RuntimeCondition,
} = import? "types.mrld";

export let World = World;

impl World {
    set_block: (pos: mc_vector, id: string) => {
        command("setblock " + string(pos.x) + " " + string(pos.y) + " " + string(pos.z) + " " + id);
    },
    if_block: (pos: mc_vector, id: string) => {
        return RuntimeCondition:IsBlock {pos, id};
    },
}



