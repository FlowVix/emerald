

struct Person {
    name: string,
    age: number,
}

impl Person {
    func baby(name: string) {
        return Person::{name, age: 0};
    }
    func speak(self) {
        print("I am ", self.name);
    }
}


struct Person {
    name: string,
    age: number,
}

impl Person {
    baby: (name: string) {
        return Person::{name, age: 0};
    }
    speak: (self) {
        print("I am ", self.name);
    }
}










