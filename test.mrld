


extract import? "libraries/std/lib.mrld";

impl MCFloat {
    sqrt: (self) -> MCFloat {
        let x = MCFloat::new(1);
        let root_calc = !{
            x = (x + self / x);
            x /= 2;
        }
        for _ of 0..5 {
            root_calc!;
        }
        return x;
    }
}

MC::print([MCFloat::new(64).sqrt()]);

